{
    "name": "yahaaylabs/gis.ph-sdk-php",
    "description": "Official PHP SDK for api.gis.ph, with first-class support for Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "Gerald Villacarlos",
            "email": "gerald@yahaay.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "GisPh\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GisPh\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GisPh\\Laravel\\GisPhServiceProvider"
            ],
            "aliases": {
                "GisPh": "GisPh\\Laravel\\Facades\\GisPh"
            }
        }
    }
}